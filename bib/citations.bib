@ARTICLE{selected_cite_1,
  title     = "Pooled screening of {CAR} {T} cells identifies diverse immune
               signaling domains for next-generation immunotherapies",
  author    = "Goodman, Daniel B and Azimi, Camillia S and Kearns, Kendall and
               Talbot, Alexis and Garakani, Kiavash and Garcia, Julie and
               Patel, Nisarg and Hwang, Byungjin and Lee, David and Park, Emily
               and Vykunta, Vivasvan S and Shy, Brian R and Ye, Chun Jimmie and
               Eyquem, Justin and Marson, Alexander and Bluestone, Jeffrey A
               and Roybal, Kole T",
  journal   = "Science Translational Medicine",
  publisher = "science.org",
  presort = {001},
  volume    =  14,
  number    =  670,
  pages     = "eabm1463",
  month     =  nov,
  year      =  2022,
  keywords = {selected},
  note = "Chimeric antigen receptors (CARs) repurpose natural signaling components to retarget T cells to cancer, but have shown limited efficacy in persistent, recurrent malignancies. To rapidly identify new CAR designs with clinical potential, I developed a multiplexed method called CAR Pooling. Forty CARs, with signaling domains derived from a range of immune cell lineages, were evaluated in pooled assays for their ability to stimulate critical T cell effector functions during repetitive stimulation that mimics long-term tumor antigen exposure. Several domains were identified that enhanced CAR function relative to clinical benchmarks after prolonged antigen stimulation, and in particular CAR T cells which used the signaling domain from the B cell receptor for BAFF (BAFF-R) were enriched for a highly cytotoxic transcriptional signature previously associated with positive clinical outcomes. I also showed that replacing the 4-1BB intracellular signaling domain with BAFF-R in a B cell maturation antigen (BCMA)–specific CAR resulted in enhanced activity in a xenotransplant model of multiple myeloma. These results demonstrate that multiplexed design and measurement of engineered receptors is a powerful strategy for understanding immune signaling and developing new and enhanced cellular therapies. \medskip \\ IP generated from this work, along with patented genome-editing methods developed with the Marson lab, became foundational technologies with which my postdoctoral advisors, Prof. Roybal and Prof Marson, co-founded Arsenal Biosciences. From 2018 to 2020 I served as a scientific consultant at Arsenal, helping to optimize and scale these tools into an exciting non-viral pooled T cell gene editing platform, which has developed new CAR-T therapies that are currently in clinical trials.",
  author+an = {1=highlight, first; 2=first}
}


@article{selected_cite_2,
  title={Composability of regulatory sequences controlling transcription and translation in Escherichia coli},
  author={Kosuri, Sriram and Goodman, Daniel B and Cambray, Guillaume and Mutalik, Vivek K and Gao, Yuan and Arkin, Adam P and Endy, Drew and Church, George M},
  journal={Proceedings of the National Academy of Sciences},
  volume={110},
  number={34},
  presort = {002},
  pages={14024--14029},
  year={2013},
  publisher={National Academy of Sciences},
  keywords = {selected},
  note = "Much of my graduate work focused on developing methods to increase the scale and throughput with which we can design, synthesize, and measure variants of genetic elements and genomes. Advancements in DNA synthesis now allow for the generation of complex pools of synthetic oligonucleotides, hundreds of bases long, which can be inserted into larger genetic systems and assayed in multiplex. Rather than random sequences generated by error-prone PCR or degenerate nucleotides, each sequence in the pool can be individually designed. To measure the effects of introducing these variants I developed a method called FlowSeq, which can accurately measure protein expression from a large multiplexed library. FlowSeq combines fluorescence activated cell sorting (FACS) and a multiplexed barcoded DNA sequence readout to measure protein expression. Combining FlowSeq with DNA library synthesis and RNA sequencing has enabled our group and others to perform a targeted search of sequence space to answer specific biological questions, including the sequence-to-function relationships of non-coding regulatory elements, codon usage, RNA splicing, and protein activity. \medskip \\
  In work published in PNAS, we used this method to test a fundamental assumption in the synthetic biology community (and much of the broader biology community) that regulatory elements controlling gene expression can be combined rationally. Using next-generation DNA synthesis, we constructed over 12,000 RBS-promoter combinations and used our new multiplexed measurement methods to characterize the entire library in a single experiment. Overall, we found that while simple models explain much of the data, the large numbers of outliers will confound large system designs. More importantly, our new rapid synthesis and characterization methods provide an intriguing alternative to standardization or model refinement by generating and screening for desired expression levels in advance when building large genetic systems.",
  author+an =  {1=first; 2=first, highlight}
}

@article{selected_cite_3,
  title={Causes and effects of N-terminal codon bias in bacterial genes},
  author={Goodman, Daniel B and Church, George M and Kosuri, Sriram},
  journal={Science},
  volume={342},
  number={6157},
  presort = {003},
  pages={475--479},
  year={2013},
  publisher={American Association for the Advancement of Science},
  keywords = {selected},
  note= "Synonymous codon changes within genes can have substantial effects on protein expression, which have been attributed to differences in tRNA abundance and the structure of 5’ messenger RNAs, among other factors. We measured transcription and translation across libraries of over 14,000 defined genetic reporter constructs to separate and quantify the factors controlling protein expression, and identified that rare codon use at the N-terminus of genes in E. coli unexpectedly leads to a strong increase in protein translation. We then quantified the effects of individual codon use with expression changes and showed that the codons that affect expression are also selected for in E. coli genes. Furthermore, this rare codon effect is not mediated by variable ribosome elongation rates as others have suggested, but by the link between rare codon use and reduced RNA secondary structure. \medskip \\ These findings have large implications for heterologous gene expression, show that current de-facto standards for codon optimization are not ideal, and provide a straightforward method to improve them. This study was an early example of how large synthetic libraries combined with multiplexed measurements can answer outstanding genome-scale questions that are often difficult to address by analyzing natural sequence alone.",
  author+an =  {1=first, highlight}
}

