@article{selected_cite_5,
  title={Optimizing complex phenotypes through model-guided multiplex genome engineering},
  author={Kuznetsov, Gleb and Goodman, Daniel B and Filsinger, Gabriel T and Landon, Matthieu and Rohland, Nadin and Aach, John and Lajoie, Marc J and Church, George M},
  journal={Genome Biology},
  volume={18},
  pages={1--12},
  presort = {005},
  label={3b},
  year={2017},
  publisher={BioMed Central},
  keywords = "selected",
  note="Modifying and sequencing genomic variants has become faster and cheaper, to the extent that individual E. coli genomes, when sequenced in bulk, cost under 5 USD each. At the scale of hundreds of these genomes, identifying and annotating the functions of mutations, comparing them among multiple clones, and tracing the genotypes and phenotypes across iteratively engineered strains becomes unwieldy without an integrated computational tool. To solve this issue, we built Millstone, a cloud-based platform for multiplex mutation analysis and iterative genome engineering. Millstone integrates alignment, variant-calling, genotype comparison, and visualization for hundreds of microbial genomic samples. In a paper published in Genome Biology, we show how Millstone has been used for in-house genome engineering projects and also describe its use in reanalyzing a published genome evolution dataset composed of over 100 experimentally evolved strains of E. coli.",
  author+an = {1=first; 2=highlight, first}
}

@article{selected_cite_6,
  title={High-throughput functional variant screens via in vivo production of single-stranded DNA},
  author={Schubert, Max G and Goodman, Daniel B and Wannier, Timothy M and Kaur, Divjot and Farzadfard, Fahim and Lu, Timothy K and Shipman, Seth L and Church, George M},
  journal={Proceedings of the National Academy of Sciences},
  volume={118},
  number={18},
  presort = {006},
  pages={e2018181118},
  year={2021},
  publisher={National Academy of Sciences},
  keywords = {selected},
  note = "To enable both multiplexed modification and measurement of bacterial genomes, I developed a method for the pooled construction of mutants bearing precise genomic sequence variations, where the barcodes allowed for multiplex phenotypic characterization using next-generation sequencing (NGS). Unlike existing techniques depending on CRISPR-Cas–directed genomic breaks for genome editing, this strategy instead used single-stranded DNA produced by a retron element for recombineering. This novel and powerful approach enables libraries of millions of elements to be constructed and offers relaxed design constraints which permit natural DNA or random variation to be used as inputs.",
  author+an = {1=first; 2=first, highlight}
}

