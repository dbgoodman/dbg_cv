@article{Goodman2022-is,
 abstract = {Chimeric antigen receptors (CARs) repurpose natural signaling
components to retarget T cells to refractory cancers but have
shown limited efficacy in persistent, recurrent malignancies.
Here, we introduce “CAR Pooling,” a multiplexed approach to
rapidly identify CAR designs with clinical potential. Forty CARs
with signaling domains derived from a range of immune cell
lineages were evaluated in pooled assays for their ability to
stimulate critical T cell effector functions during repetitive
stimulation that mimics long-term tumor antigen exposure. Several
domains were identified from the tumor necrosis factor (TNF)
receptor family that have been primarily associated with B cells.
CD40 enhanced proliferation, whereas B cell–activating factor
receptor (BAFF-R) and transmembrane activator and CAML interactor
(TACI) promoted cytotoxicity. These functions were enhanced
relative to clinical benchmarks after prolonged antigen
stimulation, and CAR T cell signaling through these domains fell
into distinct states of memory, cytotoxicity, and metabolism.
BAFF-R CAR T cells were enriched for a highly cytotoxic
transcriptional signature previously associated with positive
clinical outcomes. We also observed that replacing the 4-1BB
intracellular signaling domain with the BAFF-R signaling domain in
a clinically validated B cell maturation antigen (BCMA)–specific
CAR resulted in enhanced activity in a xenotransplant model of
multiple myeloma. Together, these results show that CAR Pooling is
a general approach for rapid exploration of CAR architecture and
activity to improve the efficacy of CAR T cell therapies.
Description CAR Pooling is a high-throughput approach to screening
and testing CAR designs with the potential to enhance engineered T
cell therapies for cancer. Test driving CAR T cells CAR T cells
have already been through multiple generations, with each
iteration improving signaling through the receptor. However, a
strategy to broadly test multiple candidate intracellular
signaling domains would allow for even better optimization. Here,
Goodman et al. have developed CAR Pooling, a multiplexed approach
to rapidly identify the top CAR signaling domains. The authors
found using CAR Pooling that the intracellular signaling domain
from BAFF-R improved CAR T cell activity; these results translated
into control of tumor burden in vivo. Thus, CAR Pooling represents
an effective approach to rapidly identify strategies to improve
CAR T cells.},
 author = {Goodman, Daniel B and Azimi, C and Kearns, Kendall and Talbot,
Alexis and Garakani, Kiavash and Garcia, Julie M and Patel, Nisarg
and Hwang, Byungjin and Lee, David S and Park, E and Vykunta,
Vivasvan S and Shy, B and Ye, Chun Jimmie and Eyquem, Justin and
Marson, A and Bluestone, J and Roybal, Kole T},
 author+an = {1=highlight, first; 2=first},
 doi = {10.1126/scitranslmed.abm1463},
 journal = {Sci. Transl. Med.},
 keywords = {first_author, journal, talk;Citations, web_selected},
 month = {9~November},
 note = {Chimeric antigen receptors (CARs) repurpose natural signaling components to retarget T cells to cancer, but have shown limited efficacy in persistent, recurrent malignancies. To rapidly identify new CAR designs with clinical potential, I developed a multiplexed method called CAR Pooling. Forty CARs, with signaling domains derived from a range of immune cell lineages, were evaluated in pooled assays for their ability to stimulate critical T cell effector functions during repetitive stimulation that mimics long-term tumor antigen exposure. Several domains were identified that enhanced CAR function relative to clinical benchmarks after prolonged antigen stimulation, and in particular CAR T cells which used the signaling domain from the B cell receptor for BAFF (BAFF-R) were enriched for a highly cytotoxic transcriptional signature previously associated with positive clinical outcomes. I also showed that replacing the 4-1BB intracellular signaling domain with BAFF-R in a B cell maturation antigen (BCMA)–specific CAR resulted in enhanced activity in a xenotransplant model of multiple myeloma. These results demonstrate that multiplexed design and measurement of engineered receptors is a powerful strategy for understanding immune signaling and developing new and enhanced cellular therapies. \medskip \\ IP generated from this work, along with patented genome-editing methods developed with the Marson lab, became foundational technologies with which my postdoctoral advisors, Prof. Roybal and Prof Marson, co-founded Arsenal Biosciences. From 2018 to 2020 I served as a scientific consultant at Arsenal, helping to optimize and scale these tools into an exciting non-viral pooled T cell gene editing platform, which has developed new CAR-T therapies that are currently in clinical trials.},
 pages = {eabm1463--eabm1463},
 title = {{Pooled screening of {CAR} {T} cells identifies diverse immune
signaling domains for next-generation immunotherapies}},
 url = {https://www.science.org/doi/10.1126/scitranslmed.abm1463},
 volume = {14},
 year = {2022}
}

